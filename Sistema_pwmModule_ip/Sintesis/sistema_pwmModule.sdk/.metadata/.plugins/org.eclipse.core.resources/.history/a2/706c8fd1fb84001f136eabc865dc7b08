#include "xparameters.h"
#include "xil_io.h"
#include "pwmModule_ip.h"
 

//====================================================

int main (void) {

	int pwm_out;
	int modulo = 1256;
	int duty = 800;

    xil_printf("-- Inicio del programa para validar el uso de IP cores propios --\r\n");
 
    SUM_IP_mWriteReg(XPAR_PWMMODULE_IP_0_S_AXI_BASEADDR, PWMMODULE_IP_S_AXI_SLV_REG0_OFFSET, modulo);
    SUM_IP_mWriteReg(XPAR_PWMMODULE_IP_0_S_AXI_BASEADDR, PWMMODULE_IP_S_AXI_SLV_REG1_OFFSET, duty);
    res = SUM_IP_mReadReg(XPAR_PWMMODULE_IP_0_S_AXI_BASEADDR, PWMMODULE_IP_S_AXI_SLV_REG3_OFFSET);

    xil_printf("Cuenta: %d + %d = %d\r\n", opA, opB, res);

 
}
 
